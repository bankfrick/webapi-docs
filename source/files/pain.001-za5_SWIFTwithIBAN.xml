<?xml version="1.0" encoding="UTF-8"?>
<Document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:iso:std:iso:20022:tech:xsd:pain.001.001.03">
    <CstmrCdtTrfInitn>
        <GrpHdr>
            <!-- NOTE: Change the last number to reupload the file -->
            <MsgId>SWIFT_IBAN-1001</MsgId>
            <CreDtTm>2024-05-16T08:00:00</CreDtTm>
            <NbOfTxs>1</NbOfTxs>
            <CtrlSum>100.00</CtrlSum>
            <InitgPty>
                <Nm>Your Company</Nm>
            </InitgPty>
        </GrpHdr>

        <PmtInf>
            <PmtInfId>1</PmtInfId>
            <PmtMtd>TRF</PmtMtd>
            <BtchBookg>true</BtchBookg>

            <!-- Must be today or a future date -->
            <ReqdExctnDt>2025-11-11</ReqdExctnDt>

            <Dbtr>
                <Nm>Your Customer</Nm>
            </Dbtr>

            <DbtrAcct>
                <Id>
                    <!-- NOTE: Replace with a syntactically valid IBAN of your account -->
                    <IBAN>LI12XXXX0000000000XXX</IBAN>
                </Id>
            </DbtrAcct>

            <DbtrAgt>
                <FinInstnId>
                    <!-- NOTE: Always the same Bank Frick BIC -->
                    <BIC>BFRILI22</BIC>
                </FinInstnId>
            </DbtrAgt>

            <CdtTrfTxInf>
                <PmtId>
                    <InstrId>Your unique ID_1</InstrId>
                    <EndToEndId>Your EndtoEndId_1</EndToEndId>
                </PmtId>

                <!-- No SEPA service level, no CHxx local instrument for foreign SWIFT -->
                <Amt>
                    <!-- Any currency allowed for foreign SWIFT, example EUR -->
                    <InstdAmt Ccy="EUR">100.00</InstdAmt>
                </Amt>

                <!-- CdtrAgt must come BEFORE Cdtr and CdtrAcct -->
                <CdtrAgt>
                    <FinInstnId>
                        <BIC>NCBKSAJEXXX</BIC>
                    </FinInstnId>
                </CdtrAgt>

                <Cdtr>
                    <Nm>Your Customer</Nm>
                    <!-- For foreign SWIFT payments, creditor address is recommended/required -->
                    <PstlAdr>
                        <StrtNm>King Fahd Road</StrtNm>
                        <BldgNb>120</BldgNb>
                        <PstCd>12271</PstCd>
                        <TwnNm>Riyadh</TwnNm>
                        <Ctry>SA</Ctry>
                    </PstlAdr>
                </Cdtr>

                <CdtrAcct>
                    <Id>
                        <!-- Recipient IBAN -->
                        <IBAN>SA4420000001234567891234</IBAN>
                    </Id>
                </CdtrAcct>

                <RmtInf>
                    <!-- Payment reference -->
                    <Ustrd>Payment Reference</Ustrd>
                </RmtInf>
            </CdtTrfTxInf>
        </PmtInf>
    </CstmrCdtTrfInitn>
</Document>
