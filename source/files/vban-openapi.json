          "200" : {
            "description" : "OK",
            "headers" : {
              "signature" : {
                "description" : "Cryptographic fingerprint of response body. For calculation method, see <algorithm> header.",
                "schema" : {
                  "type" : "string"
                },
                "required" : true
              },
              "algorithm" : {
                "description" : "Algorithm used for signing the response. Default: rsa-sha512",
